
# Disable/unset referer
# NOTE an empty space "" doesn't work!
referer = " "

cookies = off

# Fix performance and other ipv6 address issues
inet4_only = on
prefer-family = IPv4

# Security
httpsonly = on

# Disguise user agent
# IE 9 on Windows 7
# user_agent = Mozilla/5.0 (compatible; MSIE 9.0; Windows NT 6.1; Trident/5.0)
